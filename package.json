{
  "name": "microdata-node-gh-page",
  "version": "1.0.0",
  "description": "Project page for microdata-node",
  "scripts": {
    "build": "npm update microdata-node cheerio && npm run build-cheerio && npm run build-microdata-node",
    "build-cheerio": "browserify -s cheerio -r cheerio -o ./javascript/cheerio.js",
    "build-microdata-node": "browserify -s microdata -r microdata-node -o ./javascript/microdata-node.js"
  },
  "repository": {
    "type": "git",
    "url": "https://janpot@github.com/Janpot/microdata-node.git"
  },
  "author": "Jan Potoms",
  "license": "MIT",
  "homepage": "https://janpot.github.io/microdata-node",
  "devDependencies": {
    "browserify": "^7.0.2"
  },
  "dependencies": {
    "cheerio": "^0.18.0",
    "microdata-node": "*"
  }
}
